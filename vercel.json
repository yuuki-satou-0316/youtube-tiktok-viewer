{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_YOUTUBE_API_KEY": "@youtube_api_key",
    "VITE_DEFAULT_CHANNEL_ID": "@default_channel_id"
  }
}